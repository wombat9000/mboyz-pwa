{
  "name": "mboyz",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "serve": "ws --directory dist --spa index.html",
    "test": "ng test --source-map=false",
    "lint": "ng lint frontend --type-check",
    "ngsw-config": "node_modules/.bin/ngsw-config dist src/ngsw-config.json",
    "ngsw-copy": "cp node_modules/@angular/service-worker/ngsw-worker.js dist/",
    "prep-pwa": "npm run ngsw-config && npm run ngsw-copy",
    "build:prod": "ng build --progress=false --prod --configuration=prod && npm run prep-pwa",
    "build:staging": "ng build --progress=false --prod --configuration=staging && npm run prep-pwa",
    "build:dev": "ng build --progress=false --configuration=staging && npm run prep-pwa",
    "dev:start": "ng build --progress=false && npm run serve -- --port=4200",
    "dev:pwa": "npm run build:dev && npm run serve -- --port=8080",
    "dev:e2e": "start-server-and-test dev:start http://localhost:4200 cy:run",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:ci": "cypress run --env ci=true",
    "ci:test": "ng test --browser ChromeHeadless_no_sandbox --watch=false --progress=false --source-map=false",
    "ci:e2e": "start-server-and-test dev:start http://localhost:4200 cy:ci"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "6.0.2",
    "@angular/cdk": "^6.0.2",
    "@angular/common": "6.0.2",
    "@angular/compiler": "6.0.2",
    "@angular/core": "6.0.2",
    "@angular/forms": "6.0.2",
    "@angular/http": "6.0.2",
    "@angular/material": "^6.0.2",
    "@angular/platform-browser": "6.0.2",
    "@angular/platform-browser-dynamic": "6.0.2",
    "@angular/router": "6.0.2",
    "@angular/service-worker": "6.0.2",
    "@firebase/app-types": "^0.3.1",
    "@firebase/auth-types": "^0.3.2",
    "@firebase/firestore-types": "^0.4.1",
    "@firebase/storage-types": "^0.2.2",
    "@ngrx/effects": "^6.0.0-beta.3",
    "@ngrx/entity": "^6.0.0-beta.3",
    "@ngrx/store": "^6.0.0-beta.3",
    "@types/uuid": "^3.4.3",
    "angularfire2": "^5.0.0-rc.10",
    "core-js": "^2.5.5",
    "firebase": "^5.0.3",
    "hammerjs": "^2.0.8",
    "material-design-icons": "^3.0.1",
    "moment": "^2.22.1",
    "normalize.css": "^7.0.0",
    "rxjs": "^6.1.0",
    "rxjs-compat": "^6.1.0",
    "uuid": "^3.2.1",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.3",
    "@angular/cli": "6.0.3",
    "@angular/compiler-cli": "6.0.2",
    "@angular/language-service": "6.0.2",
    "@bahmutov/add-typescript-to-cypress": "^2.0.0",
    "@ngrx/store-devtools": "^6.0.0-beta.3",
    "@types/jasmine": "^2.8.7",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "^10.1.0",
    "codelyzer": "^4.1.0",
    "cypress": "^2.1.0",
    "firebase-tools": "^3.18.6",
    "jasmine-core": "~2.8.0",
    "jasmine-marbles": "^0.3.1",
    "karma": "^2.0.2",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.4.2",
    "karma-jasmine": "^1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-mocha-reporter": "^2.2.5",
    "local-web-server": "^2.5.2",
    "protractor": "^5.3.2",
    "rxjs-tslint-rules": "^3.17.0",
    "start-server-and-test": "^1.4.1",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "tslint-defocus": "^2.0.3",
    "typescript": "2.7.2"
  }
}
